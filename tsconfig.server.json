{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.server.tsbuildinfo",
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist/server",
    "skipLibCheck": true,
    "paths": {
      "~/*": ["./backend/src/*"]
    },
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": false
  },
  "include": ["backend/**/*.ts"]
}
